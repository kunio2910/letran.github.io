<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.letran.equipmentmanagement">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.ShowDetail_Activity"
            android:windowSoftInputMode="adjustPan|adjustResize" />
        <activity android:name=".views.Registration_Activity" />
        <activity android:name=".views.InputDevices_Activity" />
        <activity android:name=".views.MainActivity" />
        <activity android:name=".views.Personal_Activity" />
        <activity
            android:name=".views.SearchDevice_Activity"
            android:windowSoftInputMode="adjustPan|adjustResize" />
        <activity android:name=".views.Chart_Activity" />
    </application>

</manifest>